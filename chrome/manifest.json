{
  "manifest_version": 3,
  "name": "Google Docs Auto Typer",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://docs.google.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
